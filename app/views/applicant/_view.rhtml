									<%
									reas = []
									reas << 'Previously Tested' if wa.typing_waiver_1
									reas << "County Employee (#{wa.typing_waiver_2_dept} #{wa.typing_waiver_2_title})" if wa.typing_waiver_2
									%>
									- <%= reas.join(', ') %>
								<% end %>
							&nbsp;</td>
						</tr>
