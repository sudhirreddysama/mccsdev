			</div>
		</div>
	
	<% end %>
	
	<div class="tabs" id="sect-nav">
		<a href=".sect" class="active"><span>All</span></a>
		<a href="#sect-web"><span>General Info</span></a>
		<a href="#sect-emp"><span>Employment</span></a>
		<a href="#sect-edu"><span>Education</span></a>
		<a href="#sect-cer"><span>Licenses/Certifications</span></a>
		<a href="#sect-tra"><span>Training/Programs</span></a>
		<div class="clear"></div>
	</div>
	<script type="text/javascript">
		(function($) {
			$(function() {
				var sect_tabs = $('#sect-nav a');
				var sects = $('.sect');
				sect_tabs.click(function(e) {
					e.preventDefault();
					var $this = $(this);
					sect_tabs.removeClass('active');
					$this.addClass('active');
					sects.hide();
					var show = $this.attr('href');
					$(show).show();
					$('#tab-pad').css({'padding-bottom': show == '.sect' ? '0' : '800px'});
				});
			});
		})(jQuery);
	</script>
	<div class="sect" id="sect-web">
	
		<div class="viewer">
			<div class="header">Web Data</div>
			<div class="row">
				<table>
					<tr>
						<td>
							<label>Web Id:</label>
							<% if is_agency %>
								<%= wa.id %>
							<% else %>
								<a href="https://www.monroecounty.gov/hrapply/applications/view/<%= wa.id %>"><%= wa.id %></a>
							<% end %>
						</td>
						<td>
							<label>Name:</label>
							<%= wa.label %>
						&nbsp;<td>	
					</tr>
				</table>
			</div>
		
			<% if is_seasonal %>
				<div class="row">
					<table>
						<tr>
							<td><label>Are you 18 years of age or older?</label><%= yn? wa.over_18 %>&nbsp;</td>
						</tr>
					</table>
				</div>

				<div class="row">
					<table>
						<tr>
							<td><label>When can you begin work?</label><%= h wa.begin_work %>&nbsp;</td>
						</tr>
					</table>
				</div>	
					
				<div class="row">
					<table>
						<tr>
							<td><label>When do you need to end work?</label><%= h wa.end_work %>&nbsp;</td>
						</tr>
					</table>
				</div>			
			
				<div class="row">
					<table>
						<tr>
							<td><label>Can you work weekends?</label><%= yn? wa.work_weekends %>&nbsp;</td>
						</tr>
					</table>
				</div>			
			<% end %>
		
			<% if !is_seasonal %>
				<div class="row">
					<table>
