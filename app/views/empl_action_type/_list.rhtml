<table class="dg">
	<thead>
		<tr>
			<td width="1%">Actions</td>
			<th width="1%">ID</th>
			<th width="1%">Absent?</th>
			<th width="1%">Name</th>
			<th width="96%">Description</th>
		</tr>
	</thead>
	<tbody id="objs">
		<% @objs.each { |o| %>
			<tr class="<%= cycle 'odd', 'even' %>" id="obj_<%= o.id %>">
				<td class="nobr"><%= partial 'actions', :o => o %></td>
				<td><%= o.id %></td>
				<td><%= yn o.absent %></td>
				<td class="nobr"><%= link_to o.name, :action => :view, :id => o.id %></td>
				<td><%= h o.description %></td>
			</tr>
		<% } %>
	</tbody>
</table>