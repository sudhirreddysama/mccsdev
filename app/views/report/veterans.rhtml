<html>
	<head>
		<style type="text/css">

			html, body { padding: 0; margin: 0; border: 0; }
			body, td, th { font: 10px/15px Trebuchet MS, Verdana, Arial, sans-serif; }
			
			table { border-collapse: collapse; width: 100%; }
			td, th { vertical-align: top; text-align: left; padding: 2px; }
			
			th { font-weight: bold; border-top: 1px solid #000; border-bottom: 1px solid #000; }
			
			td { border-bottom: 1px solid #888; }
			
			.nobr { white-space: nowrap; }
			tr { page-break-inside: avoid; }
			
		</style>
	</head>
	<body>
		<div style="float: right;"><%= Time.now.d0? %></div>
		<h3>
			Veterans Credits Report<br />
		</h3>
		<table>
			<tr>
				<th class="nobr">SSN</th>
				<th>Name</th>
				<th class="nobr">Veteran</th>
				<th class="nobr">Verified?</th>
				<th class="nobr">Used?</th>
				<th class="nobr">Used&nbsp;Date</th>
				<th class="nobr">Points</th>
				<th class="nobr">Exam&nbsp;No</th>
				<th>Title</th>
			</tr>
			<% @objs.each { |o| %>
				<tr>
					<td class="nobr"><%= h o.ssn %></td>
					<td><%= h o.last_name %>, <%= h o.first_name %></td>
					<td class="nobr"><%= h o.veteran %></td>
					<td class="nobr"><%= 'VERIFIED' if o.veteran_verified %></td>
					<td class="nobr"><%= 'USED' if o.veteran_used %></td>
					<td class="nobr"><%= o.veteran_used_date.d0? %></td>
					<td class="nobr"><%= h o.veteran_used_points %></td>
					<td class="nobr"><%= h o.veteran_used_exam_no %></td>
					<td><%= h o.veteran_used_title %></td>
				</tr>
			<% } %>
		</table>		
	</body>
</html>
